{
  "name": "benchmark-state-management-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:playwright": "sudo docker build -f ./benchmark/Dockerfile.playwright -t benchmark-playwright . --no-cache",
    "test:playwright": "sudo rm -rf metrics-playwright && sudo ./benchmark/benchmark-script.sh playwright",
    "local:test:playwright": "NODE_TLS_REJECT_UNAUTHORIZED=0 pnpm exec playwright install && node ./benchmark/benchmark-playwright.js",
    "build:lighthouse": "sudo docker build -f ./benchmark/Dockerfile.lighthouse -t benchmark-lighthouse . --no-cache",
    "test:lighthouse": "sudo rm -rf metrics-lighthouse && sudo ./benchmark/benchmark-script.sh lighthouse",
    "local:test:lighthouse": "NODE_TLS_REJECT_UNAUTHORIZED=0 pnpm exec lighthouse install && ./benchmark/benchmark-lighthouse.js"
  },
  "dependencies": {
    "@hookstate/core": "^4.0.2",
    "@hookstate/localstored": "^4.0.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@rematch/core": "^2.2.0",
    "@rematch/persist": "^2.1.2",
    "@tanstack/react-router": "^1.120.15",
    "constate": "^3.3.3",
    "effector": "^23.3.0",
    "effector-react": "^23.3.0",
    "jotai": "^2.12.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "recoil": "^0.7.7",
    "redux-persist": "^6.0.0",
    "use-context-selector": "^2.0.0",
    "valtio": "^2.1.5",
    "web-vitals": "^5.0.2",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tanstack/router-plugin": "^1.120.15",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.5",
    "lighthouse": "^12.6.1",
    "playwright": "^1.45.0"
  }
}
